{
  "name": "szu-xiaoyoulu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "LAUNCH_EDITOR=./launch-buddycn.sh vite",
    "build": "vue-tsc -b && vite build",
    "preview": "npm run build && vite preview",
    "type-check": "vue-tsc -b --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "download-api": "node scripts/download-api.mjs",
    "generate-api": "orval --config ./orval.config.ts",
    "api": "npm run download-api && npm run generate-api",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.7",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "orval": "^8.0.0-rc.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sass": "^1.97.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-vue-inspector": "^5.3.2",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write"
    ]
  }
}
