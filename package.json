{
  "name": "szu-xiaoyoulu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "LAUNCH_EDITOR=./launch-buddycn.sh vite",
    "build": "vue-tsc -b && vite build",
    "preview": "pnpm run build && vite preview",
    "type-check": "vue-tsc -b --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:css": "stylelint 'src/**/*.{css,vue,scss}'",
    "lint:css:fix": "stylelint 'src/**/*.{css,vue,scss}' --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "download-api": "node scripts/download-api.mjs",
    "generate-api": "orval --config ./orval.config.ts && node scripts/postprocess-api.mjs",
    "api": "pnpm run download-api && pnpm run generate-api",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.1",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.7",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "orval": "^8.0.0-rc.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass": "^1.97.0",
    "stylelint": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-vue-inspector": "^5.3.2",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write"
    ]
  }
}
